resources:
- type: compute.v1.firewall
  name: address-{{ env["deployment"] }}
  properties:
    network: https://www.googleapis.com/compute/v1/projects/{{ env["project"] }}/global/networks/default
    sourceRanges: [0.0.0.0/0]
    allowed:
    - IPProtocol: tcp
      ports:
      - 21-23
      - '25'
      - '53'
      - '80'
      - '110'
      - '445'
      - '1521'
      - '3306'
      - '3389'
      - '5432'
      - '27017'
    - IPProtocol: udp
      ports:
      - '21'
      - '22'
      - '23'
      - '25'
      - '53'
      - '80'
      - '110'
      - '445'
      - '1521'
      - '3306'
      - '3389'
      - '5432'
      - '27017'
